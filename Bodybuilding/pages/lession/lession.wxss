page{
  background: #00a0e9;
}
.tab{
  height: 90rpx;
  display: flex;
  /* align-items: center; */
  line-height: 90rpx;
  background: #fff;
}
.tab text{
  width: 50%;
  text-align: center;
  border-bottom:1px solid #ccc ;
}
.tab text.fix{
  color: #00a0e9;
  border-bottom: 1px solid #00a0e9;
}
.nav{
  padding: 8px 0;
  display: flex;
  align-items: center;
  /* height: 120rpx; */
  margin-bottom: 30rpx;
  background: #fff;
  /* line-height: 120rpx; */
}
.nav text{
  display: 1;
  width: 100%;
  height: 80rpx;
  font-size: 80%;
  margin: 0 1.125%;
  text-align: center;
  line-height: 80rpx;
}
.nav text.fix{
  color: white;
  line-height: 140%;
  border-radius: 50%;
  background: #00a0e9;
}
